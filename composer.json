{
    "name": "ttskch/monolog-mailgun",
    "description": "Monolog handler for Mailgun",
    "keywords": [
        "monolog",
        "mailgun"
    ],
    "homepage": "https://github.com/ttskch/monolog-mailgun",
    "license": "MIT",
    "authors": [
        {
            "name": "Takashi Kanemoto"
        }
    ],
    "require": {
        "php": "^7.2",
        "mailgun/mailgun-php": "^2.4",
        "monolog/monolog": "^2.0",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ttskch\\Monolog\\Handler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ttskch\\Monolog\\Handler\\": [
                "tests/",
                "tests/Fake"
            ]
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
